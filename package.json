{
  "name": "angular2-redux",
  "version": "3.0.0",
  "description": "asdf",
  "config": {
    "buildDir": "./build",
    "buildDirTests": "./build_tests",
    "devHost": "localhost",
    "devPort": 3020
  },
  "scripts": {
    "typings": "typings",
    "build": "NODE_ENV=production npm run webpack",
    "clean": "rimraf $npm_package_config_buildDir && mkdir $npm_package_config_buildDir",
    "env": "env",
    "lint": "eslint --ext .js --ext .jsx ./app ./webpack && echo No linting errors.",
    "prebuild": "npm run clean",
    "prestart": "npm install",
    "pretest": "npm install && npm run lint",
    "pretest-ci": "npm install && npm run lint",
    "start": "NODE_ENV=development node dev-server ./webpack/config",
    "test": "NODE_ENV=test karma start",
    "test-ci": "NODE_ENV=test karma start --single-run",
    "webpack": "webpack --colors --progress --config ./webpack/config",
    "postinstall": "typings install"
  },
  "dependencies": {
    "angular2": "^2.0.0-beta.0",
    "brace": "^0.7.0",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.13",
    "immutable": "^3.7.5",
    "isomorphic-fetch": "^2.2.0",
    "lodash": "^3.10.1",
    "ng2-redux": "^2.0.2",
    "normalize.css": "^3.0.3",
    "redux": "^3.0.5",
    "redux-actions": "^0.8.0",
    "redux-thunk": "^1.0.2",
    "reflect-metadata": "^0.1.2",
    "rxjs": "^5.0.0-beta.0",
    "zone.js": "^0.5.10"
  },
  "devDependencies": {
    "autoprefixer-core": "^5.2.1",
    "babel-core": "^5.8.3",
    "babel-eslint": "^3.1.23",
    "babel-loader": "^5.3.1",
    "babel-plugin-rewire": "^0.1.8",
    "babel-runtime": "^5.6.15",
    "chai": "^3.0.0",
    "css-loader": "^0.15.2",
    "eslint": "^0.24.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "glob": "^5.0.13",
    "html-loader": "^0.3.0",
    "json-loader": "^0.5.2",
    "karma": "^0.13.2",
    "karma-chrome-launcher": "^0.2.0",
    "karma-cli": "0.1.0",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.0.2",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-sinon": "^1.0.4",
    "karma-source-map-support": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.5.1",
    "mocha": "^2.2.5",
    "mocha-loader": "^0.7.1",
    "node-libs-browser": "^0.5.2",
    "node-sass": "^3.2.0",
    "opn": "^3.0.2",
    "phantomjs": "^1.9.17",
    "postcss-loader": "^0.5.1",
    "rimraf": "^2.4.1",
    "sass-loader": "^1.0.2",
    "sinon": "^1.15.4",
    "source-map-support": "^0.3.2",
    "style-loader": "^0.12.3",
    "template-html-loader": "0.0.3",
    "ts-loader": "^0.7.2",
    "typescript": "^1.7.5",
    "typings": "^0.3.4",
    "webpack": "^1.10.1",
    "webpack-dev-server": "^1.10.1"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "ecmaFeatures": {
      "modules": true,
      "jsx": true
    },
    "globals": {
      "describe": true,
      "it": true,
      "sinon": true
    },
    "parser": "babel-eslint",
    "rules": {
      "strict": [
        2,
        "global"
      ],
      "indent": [
        2,
        2
      ],
      "quotes": [
        2,
        "single"
      ],
      "no-alert": 0,
      "no-underscore-dangle": 0
    }
  }
}
